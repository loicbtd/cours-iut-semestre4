<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/image_view_character_pitcture"
        android:layout_width="129dp"
        android:layout_height="136dp"
        android:layout_marginStart="44dp"
        android:layout_marginTop="272dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@mipmap/ic_launcher" />

    <TextView
        android:id="@+id/text_view_character_first_name"
        android:layout_width="73dp"
        android:layout_height="22dp"
        android:layout_marginStart="20dp"
        android:layout_marginTop="48dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/image_view_character_pitcture"
        app:layout_constraintTop_toTopOf="@+id/image_view_character_pitcture" />

    <TextView
        android:id="@+id/text_view_character_latitude"
        android:layout_width="75dp"
        android:layout_height="22dp"
        android:text="TextView"
        app:layout_constraintStart_toStartOf="@+id/text_view_character_first_name"
        app:layout_constraintTop_toBottomOf="@+id/text_view_character_first_name" />

    <TextView
        android:id="@+id/text_view_character_longitude"
        android:layout_width="118dp"
        android:layout_height="23dp"
        android:text="TextView"
        app:layout_constraintStart_toEndOf="@+id/text_view_character_latitude"
        app:layout_constraintTop_toBottomOf="@+id/text_view_character_first_name" />

    <TextView
        android:id="@+id/text_view_character_family_name"
        android:layout_width="120dp"
        android:layout_height="23dp"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/text_view_character_longitude"
        app:layout_constraintStart_toEndOf="@+id/text_view_character_first_name" />

</androidx.constraintlayout.widget.ConstraintLayout>